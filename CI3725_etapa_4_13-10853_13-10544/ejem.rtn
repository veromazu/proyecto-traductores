func factorial(number n) -> number
begin
   if n==1 then
      return 1;
   else
      return n * factorial(n-1);
    end;
end;
func epa(number x)->number
begin
with number k;
do
	k=factorial(3) + 2;
	return 2+k+x;
end;
end;
